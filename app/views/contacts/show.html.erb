<h2><%= @contact.first_name %></h2>
<h5>Last updated on: <%= @contact.friendly_updated_at %></h5>
<h4><%= @contact.last_name %></h4>
<h3><%= @contact.email %></h3>
<h3><%= @contact.phone %></h3>

<%= link_to "Edit Contact", "/contacts/#{ @contact.id }/edit" %>
<%= link_to "Delete", "/contacts/#{ @contact.id }", method: :delete %>
